/* colorui/mycomponents/my-index-nav.wxss */

.xu-custom-f{
  display: block;
  position: relative;
  box-sizing: border-box;
  top: 0;
  left: 0;
  
  width: 100%;
  
}
.xu-custom{
  position: fixed;
  width: 100%;
  min-height: 0px;
  padding-right: 220rpx;
  box-shadow: 0rpx 0rpx 0rpx;
  z-index: 9999;
  top: 0;
  background-color: white;
  color: black;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  
}


.title{
  width: calc(100% - 440rpx);
  position: absolute;
  text-align: center;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  margin: auto;
  padding-top: 10rpx;
  height: 60rpx;
  font-size: 31.8rpx;
  line-height: 60rpx;
  cursor: none;
  pointer-events: none;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  box-sizing: border-box;
  display: inline-block;
}

.ana-list{
  font-size: 55rpx !important;
  margin-left: 20rpx;
  margin-bottom: 15rpx;
  margin-top: 10rpx;
  display: inline-block;
}


 cover-view{
   line-height: initial !important;
   font-family: initial !important;
}